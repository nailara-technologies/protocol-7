# name  = base.regex
# descr = returns precompiled regex cache

$code{'base.log'}->( 1, 'compiling base regex cache...' );

return {

    'cmd_id' => qr|\d{2,15}|o,                     # a valid command(route) id
    'sid'    => qr|\d{3,17}|o,                     # a valid session id
    'usr'    => qr|[a-z][a-zA-Z0-9\-_]{2,16}|o,    # a valid username string
    'cmd'    => qr|[a-z][a-zA-Z0-9\-_]{2,22}|o,    # nailara command string

};
