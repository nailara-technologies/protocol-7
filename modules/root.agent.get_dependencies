# name   = root.agent.get_dependencies
# descr  = return agent dependencies of requested agent
# args   = <agent_name>
# return = <list_of_agents>

my $agent_name = shift;

<root.agents.dependencies> = {}
    if not exists <root.agents.dependencies>
    and ref(<root.agents.dependencies>) ne 'HASH';

return undef if not exists <root.agents.dependencies>->{$agent_name};

return <root.agents.dependencies>->{$agent_name};
