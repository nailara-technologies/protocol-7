## >:] ##

# name  = mod-test.cmd.write-blacklist-code
# descr = write blacklist subroutine, return path

my $code_path = <[mod-test.write_out_blacklist_pcode]>;

if ( not defined $code_path ) {
    return {
        'mode' => qw| false |,
        'data' => 'code file not written [ details in log ]'
    };
} else {
    return {
        'mode' => qw| true |,
        'data' => $code_path
    };
}

