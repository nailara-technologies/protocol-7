# >:]

# name  = base.cli.help
# descr = list cli commands for current agent

my $help_param = shift;

if ( defined $help_param ) {
    print $code{'base.parser.txt_box'}->( 'not implemented yet!', $help_param );
    return;
}

my @commands = sort keys %{ $data{'base'}{'cli'} };

print $code{'base.parser.txt_box'}->(
    $code{'base.parser.help_list'}->( \@commands, 'cli' ),
    "'" . <system.agent.name> . '\'-agent commands'
);

0;
