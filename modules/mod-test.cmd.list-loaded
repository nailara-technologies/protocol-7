# name  = mod-test.cmd.list-loaded
# descr = return loaded perl modules list [ %INC keys ]

return {
    'mode' => qw| size |,
    'data' => join '',
    map {"$ARG\n"}
        <[base.reverse-sort]>->(<[base.perlmod.get_loaded_INC_names]>)
    }

