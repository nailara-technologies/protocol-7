# >:]

# name  = browser.parent.init_code

<browser.playlist.content_types> //= 'html';
<browser.url.list>               //= [];
<browser.slideshow.running>      //= 0;
<browser.slideshow.autostart>    //= 0;

<[event.add_signal]>->(
    { 'signal' => 'CHLD', 'handler' => 'browser.parent.handler.sig_chld' } );

warn( "error page template '" . <browser.error_page> . "' not found!" )
    if !-f <browser.error_page>;

0;
