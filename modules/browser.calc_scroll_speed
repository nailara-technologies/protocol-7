# >:]

# name = browser.calc_scroll_speed

<browser.autoscroll.delay> //= 5;
<browser.autoscroll.delay> = 10 if <browser.autoscroll.delay> > 10;
<browser.autoscroll.delay> = 1  if <browser.autoscroll.delay> < 1;
<browser.autoscroll.delay>
    = sprintf( "%.6f", 1 / 60 / ( <browser.cfg.scroll_speed> / 10 ) );
<browser.autoscroll.fps> = sprintf( "%.2f", 1 / <browser.autoscroll.delay> );
<browser.autoscroll.cfg_delay> = <browser.autoscroll.delay>;
