# >:]

# name = web.call_plugin

my ( $plugin, @params ) = split /,/, $_[1];
if ( !defined &{$code{'plugin.web.content.' . $plugin }} ) {
    <[base.log]>->( 0, "unknown plugin ($plugin) requested");
    return <[web.format]>->('[ERROR:unknown plugin requested]');
}
return $code{'plugin.web.content.' . $plugin }->( @params );
