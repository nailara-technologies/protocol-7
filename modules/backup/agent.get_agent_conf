# >:]

# name       = agent.get_agent_conf


my $conf_filename = $data{'system'}{'conf_path'}.'/nailara.'.$_[0];
$conf_filename=~s/\.\.//g;
$conf_filename=~s/\/\//\//g;

if(open(my $conf_filehandle,'<',$conf_filename))
{
    my @config=<$conf_filehandle>;
    close($conf_filehandle);
    return \@config;
}

return undef;
