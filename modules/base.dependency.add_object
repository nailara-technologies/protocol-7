# >:]

# name  = base.dependency.add_object
# descr = add new dependency object

my $dep_obj = shift;
die 'dependency.add_object: expected a hash reference!'
  if ref($dep_obj) ne 'HASH';

my $object_id = [base.gen_id:$data{dependencies}{objects}];

$data{'dependency'}{'objects'}{$object_id} = $dep_obj;

print ":: added dep_obj : $object_id ::\n";

