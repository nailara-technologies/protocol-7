# >:]

# name  = root.agent.change_status
# descr = wrapper function for agent status change handler

my ( $agent_name, $new_status ) = @_;
die "no agent name specified"        if not defined $agent_name;
die "no new status string submitted" if not defined $new_status;

$code{'root.handler.agent_status'}->($agent_name, $new_status);
