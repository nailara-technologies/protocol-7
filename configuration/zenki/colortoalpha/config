
.:[ 'colortoalpha' graphics filter zenka ]:.

[load_config_file:'shared-params']

access.cmd.usr.cube = commands heart verify-instance list clear-buffer show-buffer
                      filter src-age reload *

colortoalpha.cache_dir   = /var/cache/colortoalpha
colortoalpha.cache_perm  = 0755
colortoalpha.cache_timeout = 30 # days

modules.load = auth net protocol io.ip chk-sum.sha chk-sum.md5 colortoalpha
[load_modules:<modules.load>]
[init_modules]

[root.drop_privs:<system.privs.user>]

[net.connect_to_cube:'ip.tcp']

[base.get_session_id]

[event.loop]
