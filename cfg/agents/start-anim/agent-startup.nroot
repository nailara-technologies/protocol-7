# >:]

# name  = agent-startup.nroot
# descr = 'start-anim' agent startup configuration used by 'nroot' agent

dependencies = cube X-11 openbox
max_subname_concurrency = 1
restart.disabled   = 1
heartbeat.disabled = 1

: nroot-init :
  start_mode = stdin-agent
  command_line_modules = Gtk3 Glib::Event
  cube.key = [base.prng.chars-anum:44]
  [nroot.callback.setup_agent_key:<instance_id>,'<agent_name>',<cube.key>]

: agent-init :
  [base.auth.set_agent_key:'<{cube.key}>']
