# >:]

# name  = agent-startup.nroot
# descr = 'weather' agent startup configuration used by 'nroot' agent

dependencies = core
start.on-demand = 1

: nroot-init :
  start_mode = stdin-agent
  core.key = [base.prng.chars-anum:44]
  [nroot.callback.setup_agent_key:<instance_id>,'<agent_name>',<core.key>]

: agent-init :
  [base.auth.set_agent_key:'<{core.key}>']

