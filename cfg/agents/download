
 .:[ simple 'download' agent ]:.

buffer.agent.log_cmd = log.msg

[load_config_file:'defaults']

access.cmd.usr.core = verify-instance ping help reload get_file \
                      get_config get_config_names get_mimetype

download.cfg.http.query_etc_hosts = 1 # <- resolver config
download.cfg.download_path        = /data/nailara/download

modules.preload = download.config config.json
[load_modules:<modules.preload>]
[init_modules]

[config.json.import_file:'download.config.json']

modules.load = auth net io.ip download config.json digest.md5
[load_modules:<modules.load>]
[init_modules]

[set_privs:'nailara']

[net.connect_to_core:'tcp/ip']

[base.get_session_id]

[event.loop]
