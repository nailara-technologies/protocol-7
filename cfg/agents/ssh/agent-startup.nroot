# >:]

# name  = agent-startup.nroot
# descr = 'ssh' agent startup configuration used by 'nroot' agent

dependencies = cube
max_concurrency = 1

: nroot-init :
  start_mode = stdin-agent
  cube.key = [base.prng.chars-anum:44]
  [nroot.callback.setup_agent_key:<instance_id>,'<agent_name>',<cube.key>]

: agent-init :
  [base.auth.set_agent_key:'<{cube.key}>']

