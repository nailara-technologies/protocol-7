
   .:[ core agent command access permission setup ]:.

                access.cmd.usr.*  =  exit help ping time clear ntime since \
                                     whoami log.msg *.subname localtime \
                                     public-key set-client-key \
                                     nailara-version layout.get_geometry \
                                     xserver.get_display \
                                     layout.assign_window \
                                     xserver.get_geometry \
                                     layout.get_coordinates \
                                     xserver.get_orientation \
                                     xserver.get_screen_size \
                                     xserver.get_stack_order

              access.cmd.usr.mpv  =  root.start_once xserver.get_mode \
                                     playlist.get_list reenc-msg.refresh \
                                     reenc-msg.finished root.notify_online \
                                     xserver.keep_below root.register_child \
                                     xserver.set_opacity \
                                     xserver.set_geometry \
                                     xserver.wait_visible \
                                     ffmpeg[mpv].rescale_video

              access.cmd.usr.ssh  =  *.ping

             access.cmd.usr.dbus  =  root.register_child

             access.cmd.usr.melt  =  ffmpeg[melt].frame_count

             access.cmd.usr.root  =  ping *.ping session-key ondemand-agent \
                                     set-initialized *.verify-instance

             access.cmd.usr.udev  =  notify.message

            access.cmd.usr.media  =  root.restart root.start_once \
                                     pdf2html.get_url xserver.bg_color \
                                     xserver.get_mode xserver.gpu_load \
                                     playlist.get_list reenc-msg.refresh \
                                     image2html.get_url melt.extract_frame \
                                     playlist.set_flags reenc-msg.finished \
                                     root.notify_online root.stop_implicit \
                                     xserver.keep_below xserver.hide_window \
                                     xserver.set_opacity \
                                     layout.get_subconfig \
                                     xserver.lower_window \
                                     xserver.raise_window \
                                     xserver.set_geometry \
                                     xserver.wait_visible \
                                     xserver.unhide_window \
                                     playlist.get_list_types \
                                     playlist.get_list_types \
                                     ffmpeg[mpv].rescale_video \
                                     playlist.entries_withheld

            access.cmd.usr.power  =  power-x11.set_states

            access.cmd.usr.taeki  =  ** ..*.**

           access.cmd.usr.config  =  get_config *.get_config \
                                     *.get_config_names

           access.cmd.usr.events  =  playlist.update root.start_once \
                                     layout.switch_to layout.add_overlay \
                                     root.stop_implicit \
                                     layout.remove_overlay \
                                     layout.select_default

           access.cmd.usr.layout  =  root.start notify.warn root.restart \
                                     media.slow_down playlist.update \
                                     root.start_once xserver.bg_color \
                                     xserver.fade_out browser.slow_down \
                                     playlist.get_list root.notify_online \
                                     root.stop_implicit root.notify_offline \
                                     ticker-gtk.slow_down \
                                     xserver.lower_window \
                                     root.get_instance_ids \
                                     xserver.get_window_ids \
                                     xserver.get_stack_order \
                                     screenshot.get_region_color

           access.cmd.usr.notify  =  dbus.socket_address

           access.cmd.usr.system  =  root.stop root.start root.restart \
                                     root.pid_instance root.notify_online \
                                     xserver.get_driver root.register_child \
                                     xserver.get_xorg_pid \
                                     root.force_mod_reload

           access.cmd.usr.webcam  =  playlist.get_list xserver.wait_visible

          access.cmd.usr.browser  =  pdf2html.get_url xserver.bg_color \
                                     xserver.gpu_load image2html.get_url \
                                     playlist.set_flags layout.get_subconfig \
                                     xserver.lower_window \
                                     xserver.raise_window \
                                     xserver.wait_visible \
                                     playlist.get_list_types \
                                     playlist.entries_withheld

          access.cmd.usr.compton  =  xserver.get_mode xserver.get_driver

          access.cmd.usr.fan-int  =  power.on power.off

          access.cmd.usr.openbox  =  root.register_child

          access.cmd.usr.xserver  =  root.restart root.start_once \
                                     root.register_child \
                                     layout.gpu_load_alert \
                                     xpointer.blank_cursor \
                                     system.reload_gfx_module

         access.cmd.usr.osd-logo  =  download.get_file root.stop_implicit \
                                     colortoalpha.filter xserver.get_display

         access.cmd.usr.playlist  =  root.restart weather.get_urls \
                                     *.playlist_update notify.msg_reload \
                                     download.get_mimetype \
                                     events.get_playlist_hidden \
                                     events.get_playlist_exclusive

        access.cmd.usr.pong-game  =  xserver.wait_visible \
                                     xserver.get_coordinates

        access.cmd.usr.power-x11  =  power.on power.off power.show_all

        access.cmd.usr.reenc-msg  =  root.stop_implicit

        access.cmd.usr.unix-root  =  ** ..*.**

       access.cmd.usr.image2html  =  xserver.bg_color

       access.cmd.usr.impressive  =  root.start_once playlist.get_list \
                                     root.notify_online root.stop_implicit \
                                     root.register_child xserver.set_opacity \
                                     layout.get_subconfig \
                                     xserver.raise_window \
                                     xserver.wait_visible

       access.cmd.usr.loadsplash  =  xserver.wait_visible

       access.cmd.usr.notify-osd  =  dbus.socket_address

       access.cmd.usr.rss-ticker  =  playlist.get_list ticker-gtk.read_file \
                                     ticker-sdl.read_file

       access.cmd.usr.ticker-gtk  =  layout.get_layer xserver.gpu_load \
                                     root.notify_online layout.get_subconfig \
                                     rss-ticker.resend_update

       access.cmd.usr.ticker-sdl  =  root.notify_online xserver.keep_above \
                                     xserver.set_gravity xserver.set_opacity \
                                     xserver.raise_window \
                                     xserver.wait_visible \
                                     rss-ticker.resend_update

       access.cmd.usr.unix-admin  =  ** ..*.**

       access.cmd.usr.unix-taeki  =  ** ..*.**

      access.cmd.usr.notify-info  =  notify.info

      access.cmd.usr.notify-warn  =  notify.warn

      access.cmd.usr.storchencam  =  xserver.wait_visible

  access.cmd.usr.playlist-update  =  root.restart playlist.update \
                                     layout.config_name playlist.reload_file

