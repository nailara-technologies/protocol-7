
 .:[ nailara 'log' (writer) agent ]:.

[load_config_file:'shared-params']

log.timestamp_mode = ntime # [ntime|unix|localtime]

modules.load = auth net io.ip log

debug.subs = del dump dump-env echo exec-sub get \
             list-subs set since sleep unload-dbg-mod

access.cmd.usr.core = help ping reload cur-pid verify-instance list-logs \
                      msg file-sizes list show-buffer <debug.subs>

[load_modules:<modules.load>]
[init_modules]

[set_privs:'nailara']

[net.connect_to_core:'tcp/ip']
[base.get_session_id]

[event.loop]
