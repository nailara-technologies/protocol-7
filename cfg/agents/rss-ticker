
  .:[ 'rss-ticker' fetching agent ]:.

[set_privs:'nailara']

buffer.agent.log_cmd = log.msg

[load_config_file:'defaults']

access.cmd.usr.core = help message ping reload verify-instance show-buffer \
                      get_config_names get_config subname playlist_update \
                      resend_update

rss.data.tmp_path          = /tmp/
rss.ticker.update_commands = ticker-gtk.read_file ticker-sdl.read_file

[load_config_file:'config.rss-ticker.default'] # <- .import!

rss.ticker.join_type      = mixed # [mixed|seperate]

rss.ticker.update_interval = 120 # seconds
rss.ticker.cache_timeout   = 3   # hours
rss.ticker.request_timeout = 5   # seconds

rss.ticker.trailing_seperator = 0

modules.preload = rss-ticker.config config.json
[load_modules:<modules.preload>]
[init_modules]

[config.json.import_file:'rss-ticker.config.json']
[config.json.import_file:'rss-ticker.content.json']

modules.load = auth net io.ip digest rss.ticker config.json
[load_modules:<modules.load>]
[init_modules]

[net.connect_to_core:'tcp/ip']
[base.get_session_id]

[rss.ticker.get_feed_urls]

[event.loop]
