
.:[ 'colortoalpha' graphics filter agent ]:.

buffer.agent.log_cmd = history.append

[load_config_file:'shared-params']

access.cmd.usr.core = help heart verify-instance list clear-buffer show-buffer
                      filter source-age reload *

colortoalpha.cache_dir   = /var/cache/colortoalpha
colortoalpha.cache_perm  = 0755
colortoalpha.cache_timeout = 30 # days

modules.load = auth net io.ip digest.sha digest.md5 colortoalpha
[load_modules:<modules.load>]
[init_modules]

[set_privs:<system.privs.user>]

[net.connect_to_core:'ip.tcp']

[base.get_session_id]

[event.loop]
