
.:[ nailara 'root' process management agent ]:.

buffer.system.log_cmd = log.msg

[load_config_file:'defaults']
[load_config_file:'desktop']

modules.load = root jobqueue net io.ip auth.agent \
                    config.json digest.md5 digest.sha crypt.random

load.spawn_config = 'example' # ( see ./conf/root.spawn_config.example )

access.cmd.usr.core = *

root.cfg.install_bin_unx = yes
root.cfg.unx_bin_path = '/usr/local/bin/unx'

[load_modules:'config.json root.config']
[init_modules]

[load_modules:<modules.load>]
[init_modules]

system.internal_verbosity = 2

[root.call_cmd:<system.args>]

[event.loop]
