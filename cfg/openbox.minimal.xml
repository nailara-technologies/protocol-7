<?xml version="1.0" encoding="UTF-8"?>
<openbox_config xmlns="http://openbox.org/3.4/rc"
		xmlns:xi="http://www.w3.org/2001/XInclude">
    <keyboard></keyboard>
    <mouse></mouse>
    <theme><keepBorder>no</keepBorder></theme>
    <desktops>
      <number>2</number>
      <firstdesk>1</firstdesk>
      <popupTime>0</popupTime>
      <names>
        <name>main [visible]</name>
        <name>second [hidden]</name>
      </names>
    </desktops>
    <applications>
      <application class="*"><decor>no</decor><desktop>1</desktop></application>
      <application class="impressive.*"><iconic>yes</iconic></application>
    </applications>
</openbox_config>
