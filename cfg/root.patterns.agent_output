# >:]

# name  = root.patterns.agent_output
# descr = agent startup messages and corresponding state

.: output patterns :.

  - core:/changed priviledges to user '[^\']+'$/
    [agent.change_status:<instance_id>,'online'];

  - core:/^\[(\d+)\] '([^']+)' agent session authorized.$/
    [root.handler.core_session_auth:<instance_id>,<match_1>,'agent',<match_2>]
  - core:/^\[(\d+)\] session authorized as '([^']+)'( \[u:[^\]]+\])?$/
    [root.handler.core_session_auth:<instance_id>,<match_1>,'unix',<match_2>]
  - core:/^\[(\d+)\] user session authorized to '([^']+)'$/
    [root.handler.core_session_auth:<instance_id>,<match_1>,'pwd',<match_2>]

  - core:/^\[(\d+)\] connection closed.$/
    [root.handler.core_session_close:<instance_id>,<match_1>]

  - !core:/^core session id received \[(\d+)\]$/
    [agent.set_core_sid:<instance_id>,<match_1>]

  - !core:/^instance verification string : '([a-f\d]+)'$/
    [root.handler.instance_verification_string:<instance_id>,<match_1>]
