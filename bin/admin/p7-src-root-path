#!/usr/bin/perl

use v5.24;
use English;
use Cwd qw| abs_path |;
use File::Spec::Functions;
use FindBin qw| $RealBin |;
use File::Spec::Functions qw| rel2abs |;
our $up_dir    = File::Spec->updir;
our $root_path = abs_path( rel2abs( catdir( $RealBin, ($up_dir) x 2 ) ) );

say sprintf qw| %s/ |, $root_path;    ##  protocol-7 sourcecode root path  ##

