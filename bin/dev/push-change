#!/bin/bash

echo ':' && \
git push darksun $BASH_ARGV -f && \
git push aiya-wasu $BASH_ARGV -f && \
git push darksun --prune --tags -f && \
git push aiya-wasu --prune --tags -f && \
echo ': done.'

