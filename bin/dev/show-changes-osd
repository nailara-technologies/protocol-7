watch -c --no-title -n17 "git status | grep -v git |
    /usr/bin/aosd_cat -p 2 -l 13 -R darkcyan \
    -S darkblue -n 'White Rabbit' -y 24 -x -13 \
    -f 13777 -o 17777 -u 17777 -r 127 -s 3 -a 17777"
