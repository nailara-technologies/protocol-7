# >:]

# name  = root.patterns.agent_output
# descr = agent startup messages and corresponding state

.: output patterns :.

  - core:/tcp server \[[^\]]+\] launched\.$/
    [root.agent.change_status:'<agent_name>','online'];
    [log.ok:"agent '<agent_name>' is online."]

  - core:/failed to bind protocol 'nailara'/
    [root.agent.change_status:'<agent_name>','failed'];
