
  .:[ pdf2html agent ]:.

buffer.system.log_cmd = log.msg

[load_config_file:'defaults']

access.cmd.usr.core = help ping reload verify_instance list dump_buffer get_url

access.cmd.usr.parent = help reload dump_buffer convert_pdf

pdf.html.cache_dir = /var/cache/pdf2html
pdf.html.cache_timeout = 30 # days

modules.load = auth net io.ip digest.sha digest.sha1 pdf.html.base
[load_modules:<modules.load>]
[init_modules]

[set_privs:<system.privs.user>]

[net.connect_to_core:'tcp/ip']

[pdf.html.base.fork_conv_child]

[event.loop]
