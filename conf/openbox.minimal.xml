<?xml version="1.0" encoding="UTF-8"?>
<openbox_config xmlns="http://openbox.org/3.4/rc"
		xmlns:xi="http://www.w3.org/2001/XInclude">
    <desktops>
      <number>2</number>
      <firstdesk>1</firstdesk>
      <popupTime>0</popupTime>
      <names>
        <name>main</name>
        <name>hidden</name>
      </names>
    </desktops>
    <applications>
      <application class="*">
        <decor>no</decor>
        <desktop>1</desktop>
      </application>
    </applications>
    <keyboard></keyboard>
    <mouse></mouse>
    <theme><keepBorder>no</keepBorder></theme>
</openbox_config>
