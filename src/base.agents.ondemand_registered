# >:]

# name = base.agents.ondemand_registered

my $agent_name = shift;

map {
    return $ARG
        if <agents.virtual>->{$ARG}->{'name'} eq $agent_name
        and <agents.virtual>->{$ARG}->{'type'} eq 'ondemand'
    }
    keys %{<agents.virtual>};

return 0;

