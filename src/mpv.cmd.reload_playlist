# >:]

# name  = mpv.cmd.reload_playlist
# descr = re-fetch list of files from playlist agent
# todo  = implement error handling...

<[mpv.get_playlist]>->('video');

return { 'mode' => 'ack', 'data' => 'playlist reloaded' }

