# >:]

# name  = chk-sum.jha.b32
# descr = returns base64 url encoded [ network ordered ] jhash value

return Crypt::Misc::encode_b32r(
    pack( 'N', Digest::JHash::jhash( join( '', @_ ) ) ) )

