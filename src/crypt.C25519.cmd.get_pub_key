# >:]

# name  = crypt.C25519.cmd.get_pub_key
# descr = return default C25519 public key [b64u]

return {
    'mode' => 'nak',
    'data' => 'C25519 default public key is not defined!'
    }
    if not defined $KEYS{'C25519'}{'default'}{'pub'};

return {
    'mode' => 'ack',
    'data' => encode_base64url( $KEYS{'C25519'}{'default'}{'pub'} )
};
