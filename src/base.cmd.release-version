# >:]

# name  = base.cmd.release-version
# descr = return the nailara release version string

return {
    'mode' => 'nak',
    'data' => 'release version is undefined'
    }
    if not defined <nailara.release_version>;

return { 'mode' => 'ack', 'data' => <nailara.release_version> }

