# >:]

# name  = base.cmd.release-version
# descr = return the nailara release version string

return { 'mode' => 'nak', 'data' => 'not defined' }
    if not defined <protocol-7.release-version>;

return {
    'mode' => 'ack',
    'data' => ': AMOS release version : ' . <protocol-7.release-version>
    }

