# >:]

# name  = base.sig_trap
# descr = load debug module in running agent

if ( ref( $code{'debug.dump'} ) eq 'CODE' ) {
    <[base.log]>->( 1, "[SIGTRAP] 'debug' routines already present" );
} else {
    <[base.log]>->( 0, "[SIGTRAP] loading 'debug' source code ..," );

    <[base.load_modules]>->('debug');
    <[base.init_modules]>->('debug');

}

