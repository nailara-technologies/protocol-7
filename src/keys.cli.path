# >:]

# name  = keys.cli.path
# descr = return current user's C25519 key directory path
# note  = nailara keys path | grep -v ':$' | sed 's/.*: *//'

my $key_path = <keys.key_dir_path> // '';
my $key_usr  = <crypt.C25519.key_uid_user>;

<[base.log]>->( 1, "returning $key_usr\'s key path ['$key_path']" );

printf ":\n: nailara key path [$key_usr] : %s\n:\n", $key_path;
