# >:]

# name  = xserver.cmd.disable_dpms
# descr = disable display power saving modes

return { 'mode' => 'nak', 'data' => 'xserver has no DPMS support' }
    if !<xserver.has_dpms>;

<xserver.x11>->DPMSSetTimeouts( 0, 0, 0 );
<xserver.x11>->DPMSInfo();
<xserver.x11>->DPMSDisable();
<xserver.x11>->DPMSInfo();

<[base.log]>->( 1, "DPMS manually disabled" );

return { 'mode' => 'ack', 'data' => 'DPMS now disabled' };
