# >:]

# name  = io.unix.socket.output.close

if ( -S $_[0] ) {
    close( $_[0] );
    return 0;
} else {
    <[base.log]>->(
        0, 'io.unix.socket.output.close: parameter is not a socket'
    );
    return 1;
}

