# >:]

# name = universal.stop_loadsplash

<[base.log]>->( 1, 'stopping agent ' . <universal.lsp_name> );
<[base.proto.nailara.command.send.local]>->(
    {   'command'   => 'core.nroot.stop_implicit',
        'call_args' => { 'args' => <universal.lsp_name> }
    }
);
<universal.loadsplash_running> = 0;

