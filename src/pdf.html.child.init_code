<[base.perlmod.autoload]>->('Graphics::Magick');

<pdf.html.conversions> //= {};

<pdf.html.cache_timer>->cancel if exists <pdf.html.cache_timer>;
<pdf.html.cache_timer> = <[event.add_timer]>->(
    {   'after'    => 120,
        'interval' => 3600 * 24,
        'handler'  => 'pdf.html.child.check_cache_timeout'
    }
);

0;

