# >:]

# name  = xserver.cmd.enable_dpms
# descr = [re]activate display power saving modes

return { 'mode' => 'nack', 'data' => 'xserver has no DPMS support' }
    if !<xserver.has_dpms>;

<xserver.x11>->DPMSEnable();
<xserver.x11>->DPMSInfo();
<xserver.x11>->DPMSInfo();

return { 'mode' => 'ack', 'data' => 'DPMS enabled' };
