# >:]

# name  = composition.cmd.reset-tile-group
# descr = switch back to 'fallback' tile group

<[base.log]>->( 1, "switching to 'fallback' tile group..," );
return
    <[composition.cmd.switch-tile-group]>
    ->( { 'args' => <composition.fallback_tile_group> } );

