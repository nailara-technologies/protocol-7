# >:]

# name  = browser.cmd.scroll_start
# descr = automatically scroll down web page

my $reply_id = $$call{'reply_id'};

<window.scrolling> //= 0;
return { 'mode' => 'nak', 'data' => 'already scrolling' }
    if <window.scrolling>;

<[browser.scroll_start]>->($reply_id);

return { 'mode' => 'later' };
