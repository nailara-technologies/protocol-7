# >:]

# name  = layout.cmd.default_layout
# descr = return configured default layout name

return { 'mode' => 'nak', 'data' => 'default_layout is undefined!' }
    if not defined <layout.default_layout>;

return { 'mode' => 'ack', 'data' => <layout.default_layout> }

