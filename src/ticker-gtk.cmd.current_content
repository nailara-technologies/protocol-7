# >:]

# name  = ticker-gtk.cmd.current_content
# descr = return currently loaded content string

return {
    'mode' => 'nak',
    'data' => "no loaded ticker content in memory"
    }
    if not defined <ticker-gtk.content.txt>;

return {
    'mode' => 'ack',
    'data' => sprintf( '"%s"', <ticker-gtk.content.txt> )
    }
