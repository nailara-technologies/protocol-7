# >:]

# name  = mpv.cmd.pause
# descr = pause playback

push( @{<mpv.reply_ids>}, $$call{'reply_id'} );

push(
    @{<mpv.command.reply>},
    { 'handler' => 'mpv.handler.cmd_pause_property' }
);
<mpv.pause_cmd_mode> = 'pause';
<[mpv.send_command]>->( 'get_property_string', 'pause' );

return { 'mode' => 'later' };
