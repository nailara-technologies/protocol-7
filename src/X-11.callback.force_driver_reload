# >:]

# name = X-11.callback.force_driver_reload

<[base.log]>->(
    1, "(!) requesting forced gfx driver reload from system agent ..."
);

<[base.protocol-7.command.send.local]>
    ->( { 'command' => 'core.system.reload_gfx_module' } );

