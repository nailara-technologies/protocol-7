# >:]

# name  = storchencam.cmd.enable_time
# descr = enable time display (t.r. corner)

return { 'mode' => 'ack', 'data' => 'already enabled' }
    if <storchencam.display_time>;

<[base.log]>->( 1, "manually enabling time display" );
<storchencam.display_time> = 1;
return { 'mode' => 'ack', 'data' => 'time display enabled' }

