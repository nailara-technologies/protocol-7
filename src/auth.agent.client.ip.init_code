# >:]

# name  = auth.agent.client.ip.init_code

if (    exists $data{'agent'}
    and exists <agent.init>
    and defined <agent.init.session_key>
    and <agent.init.session_key> ne ''
    and not defined $KEYS{'agent'}{'session_key'} ) {
    <[base.log]>->( 2, '[client.ip] setting agent auth session key .,' );
    $KEYS{'agent'}{'session_key'} = <agent.init.session_key>;
    undef <agent.init.session_key>;
}

return 0;

