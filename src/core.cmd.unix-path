# >:]

# name  = core.cmd.unix-path
# descr = return unix domain socket path to core agent

return { 'mode' => 'nak', 'data' => 'not defined' } if not defined <unix.path>;

return { 'mode' => 'ack', 'data' => '/var/run/.7/UNIX/' . <unix.path> }

