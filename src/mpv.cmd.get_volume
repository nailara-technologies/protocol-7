# >:]

# name  = mpv.cmd.get_volume
# descr = return current volume setting [in percent]

push( @{<mpv.reply_ids>}, $$call{'reply_id'} );

push( @{<mpv.command.reply>}, { 'handler' => 'mpv.handler.pipe.volume' } );
<[mpv.send_command]>->( 'get_property_string', 'volume' );

return { 'mode' => 'later' };
