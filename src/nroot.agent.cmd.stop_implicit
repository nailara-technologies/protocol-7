# >:]

# name  = nroot.agent.cmd.stop_implicit
# param = <agent[s]|inst.>
# descr = stop agent instance[s] [autom. restartable]

$_[0]->{'mode'} = 'implicit';

return <[agent.cmd.stop]>->(@_);

