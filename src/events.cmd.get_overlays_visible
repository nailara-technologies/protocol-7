# >:]

# name  = events.cmd.get_overlays_visible
# descr = list visible layout overlay events

my $reply_str = '';

<events.stack.overlay.visibility> //= {};

map { $reply_str .= "$_\n" if <events.stack.overlay.visibility>->{$_} > 0 }
    sort keys %{<events.stack.overlay.visibility>};    # LLL: sort by timestamp!

return { 'mode' => 'data', 'data' => $reply_str }

