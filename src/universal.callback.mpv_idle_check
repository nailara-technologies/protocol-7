# >:]

# name = universal.callback.mpv_idle_check

<[base.log]>->( 2, "checking if mpv really resumed playback.." );

<[base.proto.nailara.command.send.local]>->(
    {   'command' => 'mpv.is_idle',
        'reply'   => { 'handler' => 'universal.handler.mpv_idle_reply' }
    }
);

