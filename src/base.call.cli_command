# >:]

# name  = base.call.cli_command
# descr = executes specified cli command or base.cli.help if no params given

( my $cli_cmd, my $cli_params ) = split( / +/, shift, 2 );

$cli_cmd //= 'help';

if ( not exists $data{'base'}{'cli'}{$cli_cmd} ) {
    print "\n  << command '$cli_cmd' does not exist [ 'help' for list >>\n\n";
    return;
}

$code{ $data{'base'}{'cli'}{$cli_cmd} }->($cli_params);

