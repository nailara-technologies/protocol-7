# >:]

# name = start-anim.wait_for_window

( my $match_str = <start-anim.window_title> ) =~ s|([\[\]])|\\$1|g;
unless ( <[base.X-11..wait_for_window]>->($match_str) ) {
    <[base.log]>->( 0, "[!] window unable to open, aborting agent startup.." );
    exit(2);
}
<[base.X-11..assign_window]>;

