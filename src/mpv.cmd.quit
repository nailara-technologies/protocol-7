# >:]

# name  = mpv.cmd.quit
# descr = initiate mpv agent shutdown

<agent.shutdown> = 1;

<[base.log]>->( 1, "quit command received.." );

push( @{<mpv.reply_ids>}, $$call{'reply_id'} );
<mpv.success_reply_str> = 'shutting down mpv agent';

<[mpv.send_command]>->('quit');

return { 'mode' => 'deferred' };
