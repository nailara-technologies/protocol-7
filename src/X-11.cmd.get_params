# >:]

# name  = X-11.cmd.get_params
# descr = return the X-server's startup parameters

if ( <X-11.mode> eq 'host' ) {
    return {
        'mode' => 'nak',
        'data' => "X-11 parameters not available in 'host' mode"
    };
} else {
    return { 'mode' => 'ack', 'data' => <X-11.params>->{<X-11.mode>} };
}

