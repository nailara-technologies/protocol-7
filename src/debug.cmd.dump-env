# >:]

# name  = debug.cmd.dump-env
# descr = dump agent's %ENV content

return {
    'mode' => 'data',
    'data' => join( '',
        map { my $val = $ENV{$_}; $val =~ s|\n|\\n|g; "$_ = $val\n" }
            keys %ENV )
};

