# >:]

# name  = debug.cmd.keep-debug-loaded
# descr = keep the debug module present after reload

<base.debug.keep_on_reload> //= 0;

return { 'mode' => 'ack', 'data' => 'already set' }
    if <base.debug.keep_on_reload>;

<base.debug.keep_on_reload> = 1;

return { 'mode' => 'ack', 'data' => 'debug module will now persist on reload' }

