# name  = cube.cmd.unix-path
# descr = return unix domain socket path to cube agent

return { 'mode' => 'nak', 'data' => 'not defined' } if not defined <unix.path>;

return { 'mode' => 'ack', 'data' => '/var/tmp/.7/UNIX/' . <unix.path> }

