# >:]

# name  = power.cmd.get_device_ids
# descr = return ids of configured devices

<power.cfg.devices> //= {};

my $reply_str = '';

map { $reply_str .= "$_\n" } sort { $a <=> $b } keys %{<power.cfg.devices>};

return { 'mode' => 'data', 'data' => $reply_str };

