# >:]

# name  = io.unix.socket.in.close

if ( -S $_[0] ) {
    close( $_[0] );
    return 0;
} else {
    <[base.log]>->( 0, 'io.unix.socket.in.close: parameter is not a socket' );
    return 1;
}

