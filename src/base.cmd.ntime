# >:]

# name  = base.cmd.ntime
# param = [fp_precision|unix_t]
# descr = return nailara network time, current or unix

my $param = $$call{'args'} // 0;

return {
    'mode' => 'nak',
    'data' => 'expected floating point precision or unix time-stamp'
    }
    if $param =~ /\D/;

return {
    'mode' => 'ack',
    'data' => <[base.ntime]>->($param)
};

