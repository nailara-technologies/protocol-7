# >:]

# name   = base.cmd.clear
# descr  = return ansi sequence to clear screen

my @smileys    = qw| =) \o/ >=P' >:] |;
my $chosen_one = int( rand( scalar @smileys ) );
$reply = {
    'mode' => 'data',
    'data' => sprintf( "%s%3s\n\n", $ANSI{'cls'}, $smileys[$chosen_one] )
};

