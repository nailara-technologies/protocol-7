# >:]

# name  = mpv.cmd.current-file
# descr = return currently loaded video file path

if ( defined <mpv.current.path> ) {
    return { 'mode' => 'ack', 'data' => <mpv.current.path> };
} else {
    return { 'mode' => 'nak', 'data' => 'no video file loaded' };
}

