# >:]

# name  = media.cmd.get_coordinates
# descr = return media agent tile coords

return {
    'mode' => 'ack',
    'data' => join( ' ',
        map { <x11.coordinates>->{$_} } ( 'left', 'top', 'right', 'bottom' ) )
    }

