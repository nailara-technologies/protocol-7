# >:]

# name  = browser.cmd.fetch_url_list
# descr = [re]fetch slideshow url list (playlist agent)
# todo  = implement error handling...

my @content_types = split( / +/, <browser.playlist.content_types> );

<[browser.get_url_list]>->(@content_types);

# LLL: reply later!

return { 'mode' => 'ack', 'data' => 'playlist reloaded' }

