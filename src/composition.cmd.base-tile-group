# >:]

# name  = composition.cmd.base-tile-group
# descr = return current 'fallback' tile group name

return { 'mode' => 'nak', 'data' => 'fallback_tile_group is undefined!' }
    if not defined <composition.fallback_tile_group>;

return { 'mode' => 'ack', 'data' => <composition.fallback_tile_group> }

