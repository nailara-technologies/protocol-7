# >:]

# name  = base.cmd.source-version
# descr = running nailara agent source code version

return { 'mode' => 'nak', 'data' => 'the source code version is not defined!' }
    if not defined <nailara.source_version>;

return { 'mode' => 'ack', 'data' => <nailara.source_version> }

