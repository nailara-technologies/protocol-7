# >:]

# name  = base.cmd.source-version
# descr = running nailara agent source code version

return { 'mode' => 'nak', 'data' => 'not defined' }
    if not defined <protocol-7.source-version>;

return {
    'mode' => 'ack',
    'data' => ': protocol-7 source version : ' . <protocol-7.source-version>
    }

