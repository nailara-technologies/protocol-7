# >:]

# name  = xserver.window_id_found

my $window_id = shift // '';
die 'expected window id parameter' if $window_id !~ m|^\d+$|;

for my $w_id (<[xserver.get_window_ids]>) {
    return 1 if $w_id == $window_id;
}

return 0;

