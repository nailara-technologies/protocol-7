# >:]

# name  = mpv.cmd.stop
# descr = stop playback

push( @{<mpv.reply_ids>}, $$call{'reply_id'} );
<mpv.success_reply_str> = 'playback stopped';

<[mpv.send_command]>->('stop');

<[base.proto.nailara.command.send.local]>
    ->( { 'command' => "core.report_paused" } )
    if <system.agent.mode> eq 'media-child';

return { 'mode' => 'later' };
