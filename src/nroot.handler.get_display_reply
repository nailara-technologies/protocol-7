# >:]

# name = nroot.handler.get_display_reply

my $reply = shift;

if ( $reply->{'cmd'} eq 'ACK' ) {
    <x11.display> = $reply->{'call_args'}->{'args'};
    <[base.log]>->(
        1, "received x11.display from xserver agent., [" . <x11.display> . "]"
    );
}

