# >:]

# name  = browser.cmd.stop_slideshow
# descr = stop the URL slideshow

return { 'mode' => 'nak', 'data' => 'slideshow is already disabled' }
    if <browser.slideshow.running> == 0;

<browser.slideshow.running> = 0;

return { 'mode' => 'ack', 'data' => 'stopping slideshow' }

