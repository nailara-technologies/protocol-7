# >:]

# name = universal.stop_start-anim

<[base.log]>->( 1, 'stopping agent ' . <universal.lsp_name> );
<[base.protocol-7.command.send.local]>->(
    {   'command'   => 'cube.nroot.stop_implicit',
        'call_args' => { 'args' => <universal.lsp_name> }
    }
);
<universal.start-anim_running> = 0;

