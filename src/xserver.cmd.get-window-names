# >:]

# name  = xserver.cmd.get-window-names
# descr = return list of window ids and titles

my $reply_str = '';
foreach my $root_id (<[xserver.get_window_ids]>) {
    my $title = <[xserver.get_window_title]>->($root_id);
    $reply_str .= "$title\n";
}

return { 'mode' => 'data', 'data' => $reply_str }

