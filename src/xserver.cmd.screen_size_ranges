# >:]

# name  = xserver.cmd.screen_size_ranges
# descr = return available screen size range

my @range = <xserver.x11>->RRGetScreenSizeRange( <xserver.x11>->root );

return {
    'mode' => 'ack',
    'data' => sprintf( "min: %dx%d max: %dx%d", @range )
    }

