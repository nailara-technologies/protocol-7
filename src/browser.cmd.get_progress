# >:]

# name  = browser.cmd.get_progress
# descr = return page loading status in percent

my $bg_index = <browser.overlay.index.bg>;
my $view     = <browser.gtk_obj.view>->{$bg_index};

return {
    'mode' => 'ack',
    'data' => sprintf( "%.2f", ( $view->get_estimated_load_progress() * 100 ) )
        . '%'
    }

