# >:]

# name = browser.callback.query_withheld_entries

<[base.log]>->( 1, "checking if playlist agent is withholding entries..." );

<[base.proto.nailara.command.send.local]>->(
    {   'command'   => "core.mlist.entries_withheld",
        'call_args' => { 'args' => <system.agent.subname> },
        'reply'     => { 'handler' => 'browser.handler.withheld_entries_reply' }
    }
);
