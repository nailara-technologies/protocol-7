# name   = nroot.agent.get_dependencies
# descr  = return agent dependencies of specified agent
# return = <list_of_agents> (as space seperated string)

my $agent_name = shift;

return undef if not exists <nroot.start_setup.agents.config>;
my $config = <nroot.start_setup.agents.config>->{$agent_name} || {};
return $config->{'dependencies'} || undef;

