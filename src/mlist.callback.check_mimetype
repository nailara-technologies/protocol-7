# name = mlist.callback.check_mimetype

my $params = shift->w->data;

<[base.proto.nailara.command.send.local]>->(
    {   'command'   => 'core.download.get_mimetype',
        'call_args' => { 'args' => $params->{'check_url'} },
        'reply'     => {
            'handler' => 'mlist.handler.mimetype_reply',
            'params'  => $params
        }
    }
);

