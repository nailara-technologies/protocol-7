# >:]

# name  = browser.cmd.enable_javascript
# descr = immediate [re]enabling of JS execution

return { 'mode' => 'nak', 'data' => 'javascript is already enabled.' }
    if <browser.cfg.javascript_enabled>;

<[base.log]>->( 0, "<<< globally [re]enabling javascript [ requested ] >>>" );

<[browser.enable_javascript]>;

return { 'mode' => 'ack', 'data' => 'javascript enabled' }

