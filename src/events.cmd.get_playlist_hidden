# >:]

# name  = events.cmd.get_playlist_hidden
# descr = hidden entry list from playlist event stack

my $reply_str = '';

map {
    $reply_str .= "$_\n"
        if defined <events.stack.playlist>->{$_}->{'action'}->{'type'}
        and <events.stack.playlist>->{$_}->{'action'}->{'type'} ne
        'playlist_exclusive_entry'
        and <events.stack.playlist>->{$_}->{'visibility'} < 1
    }
    keys %{<events.stack.playlist>};

return { 'mode' => 'raw', 'data' => $reply_str }
