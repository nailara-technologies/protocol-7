# >:]

# name  = nroot.set_up_ondemand_zenki
# descr = set up specified zenki for ondemand startup

<nroot.ondemand_zenki> = [];

foreach my $zenka_name (@_) {
    my $zenka_config   = <nroot.start_setup.zenki.config>->{$zenka_name};
    my $start_ondemand = $zenka_config->{'start'}->{'on-demand'} // 0;
    push( @{<nroot.ondemand_zenki>}, $zenka_name ) if $start_ondemand;
}

