# name = base.fork

my $pid = CORE::fork;

if ( !$pid ) {    # making rand() fork-safe..,
    ( my $seed = $PID + $code{'base.ntime'}->(13) ) =~ s|\.||;
    srand($seed);
}

return $pid;

