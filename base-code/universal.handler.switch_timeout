# >:]

# name = universal.handler.switch_timeout

my $params       = shift->w->data;
my $zenka_name   = $params->{'zenka'};
my $timeout_secs = <universal.switch_timeout>;

<[base.log]>->(
    0, "[$zenka_name] switch timeout after $timeout_secs seconds!"
);

<[universal.self_restart]>;

