# >:]

# name  = debug.cmd.dump
# param = [key_pattern]
# descr = internal program state from 'data'

my $keyword = $$call{'args'} // '';
if ( not eval {qr|$keyword|} ) {
    ( my $regex_err = lc($EVAL_ERROR) ) =~ s| in regex.+\n$||;
    return {
        'mode' => 'false',
        'data' => "$regex_err in regex [ invalid pattern ]"
    };
}

$reply = {
    'mode' => 'data',
    'data' => <[base.dump_data]>->( undef, undef, $$call{'args'} )
};

