# name  = base.context.list

my @params;

foreach my $param (@ARG) {
    if ( ref($param) eq 'HASH' and keys %{$param} ) {
        push( @params, keys %{$param} );
    } elsif ( ref($param) eq 'ARRAY' and scalar @{$param} ) {
        push( @params, @{$param} );
    } elsif ( ref($param) eq 'SCALAR' and defined ${$param} ) {
        push( @params, ${$param} );
    } else {
        push( @params, $param );
    }
}

return @params;

